{
  "name": "@peakhealth/ui",
  "version": "0.0.0",
  "type": "module",
  "description": "Peak Health Design System - A comprehensive UI component library",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/design-system.css",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node scripts/generate-exports.js && vite build",
    "generate-exports": "node scripts/generate-exports.js",
    "dev": "vite build --watch",
    "clean": "rm -rf dist exports.json",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "react-day-picker": "^9.9.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/react": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-plugin-storybook": "^9.1.3",
    "jsdom": "^26.1.0",
    "storybook": "^9.1.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "design-system",
    "ui-components",
    "react",
    "typescript",
    "peak-health",
    "fitness",
    "health"
  ],
  "author": "Peak Health Team",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./accordion": {
      "types": "./dist/components/accordion/index.d.ts",
      "import": "./dist/components/accordion/index.js",
      "require": "./dist/components/accordion/index.js"
    },
    "./alert": {
      "types": "./dist/components/alert/index.d.ts",
      "import": "./dist/components/alert/index.js",
      "require": "./dist/components/alert/index.js"
    },
    "./avatar": {
      "types": "./dist/components/avatar/index.d.ts",
      "import": "./dist/components/avatar/index.js",
      "require": "./dist/components/avatar/index.js"
    },
    "./badge": {
      "types": "./dist/components/badge/index.d.ts",
      "import": "./dist/components/badge/index.js",
      "require": "./dist/components/badge/index.js"
    },
    "./button": {
      "types": "./dist/components/button/index.d.ts",
      "import": "./dist/components/button/index.js",
      "require": "./dist/components/button/index.js"
    },
    "./calendar": {
      "types": "./dist/components/calendar/index.d.ts",
      "import": "./dist/components/calendar/index.js",
      "require": "./dist/components/calendar/index.js"
    },
    "./card": {
      "types": "./dist/components/card/index.d.ts",
      "import": "./dist/components/card/index.js",
      "require": "./dist/components/card/index.js"
    },
    "./checkbox": {
      "types": "./dist/components/checkbox/index.d.ts",
      "import": "./dist/components/checkbox/index.js",
      "require": "./dist/components/checkbox/index.js"
    },
    "./collapsible": {
      "types": "./dist/components/collapsible/index.d.ts",
      "import": "./dist/components/collapsible/index.js",
      "require": "./dist/components/collapsible/index.js"
    },
    "./dialog": {
      "types": "./dist/components/dialog/index.d.ts",
      "import": "./dist/components/dialog/index.js",
      "require": "./dist/components/dialog/index.js"
    },
    "./input": {
      "types": "./dist/components/input/index.d.ts",
      "import": "./dist/components/input/index.js",
      "require": "./dist/components/input/index.js"
    },
    "./label": {
