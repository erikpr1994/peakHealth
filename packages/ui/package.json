{
  "name": "@peakhealth/ui",
  "version": "0.0.0",
  "type": "module",
  "description": "Peak Health Design System - A comprehensive UI component library",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/design-system.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node scripts/generate-exports.js && vite build",
    "generate-exports": "node scripts/generate-exports.js",
    "dev": "vite build --watch",
    "clean": "rm -rf dist exports.json",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-onboarding": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-storybook": "^9.1.2",
    "jsdom": "^26.1.0",
    "storybook": "^9.1.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "design-system",
    "ui-components",
    "react",
    "typescript",
    "peak-health",
    "fitness",
    "health"
  ],
  "author": "Peak Health Team",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./button": {
      "types": "./dist/button.d.ts",
      "import": "./dist/button.js",
      "require": "./dist/button.js"
    },
    "./button.css": {
      "import": "./dist/assets/components/button/button.css"
    },
    "./input": {
      "types": "./dist/input.d.ts",
      "import": "./dist/input.js",
      "require": "./dist/input.js"
    },
    "./input.css": {
      "import": "./dist/assets/components/input/input.css"
    },
    "./modal": {
      "types": "./dist/modal.d.ts",
      "import": "./dist/modal.js",
      "require": "./dist/modal.js"
    },
    "./modal.css": {
      "import": "./dist/assets/components/modal/modal.css"
    },
    "./toast": {
      "types": "./dist/toast.d.ts",
      "import": "./dist/toast.js",
      "require": "./dist/toast.js"
    },
    "./toast.css": {
      "import": "./dist/assets/components/toast/toast.css"
    },
    "./tooltip": {
      "types": "./dist/tooltip.d.ts",
      "import": "./dist/tooltip.js",
      "require": "./dist/tooltip.js"
    },
    "./tooltip.css": {
      "import": "./dist/assets/components/tooltip/tooltip.css"
    },
    "./design-system": {
      "import": "./dist/design-system.css"
    },
    "./styles": {
      "import": "./dist/ui.css"
    }
  }
}
