      "require": "./dist/components/label/index.js"
    },
    "./modal": {
      "types": "./dist/components/modal/index.d.ts",
      "import": "./dist/components/modal/index.js",
      "require": "./dist/components/modal/index.js"
    },
    "./radio": {
      "types": "./dist/components/radio/index.d.ts",
      "import": "./dist/components/radio/index.js",
      "require": "./dist/components/radio/index.js"
    },
    "./select": {
      "types": "./dist/components/select/index.d.ts",
      "import": "./dist/components/select/index.js",
      "require": "./dist/components/select/index.js"
    },
    "./textarea": {
      "types": "./dist/components/textarea/index.d.ts",
      "import": "./dist/components/textarea/index.js",
      "require": "./dist/components/textarea/index.js"
    },
    "./toast": {
      "types": "./dist/components/toast/index.d.ts",
      "import": "./dist/components/toast/index.js",
      "require": "./dist/components/toast/index.js"
    },
    "./tooltip": {
      "types": "./dist/components/tooltip/index.d.ts",
      "import": "./dist/components/tooltip/index.js",
      "require": "./dist/components/tooltip/index.js"
    },
    "./design-system": {
      "import": "./dist/design-system.css"
    },
    "./components": {
      "import": "./dist/ui.css"
    }
  }
}