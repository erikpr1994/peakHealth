{
  "name": "@peakhealth/ui",
  "version": "0.0.0",
  "description": "Peak Health Design System - A comprehensive UI component library",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node scripts/generate-exports.js && vite build",
    "generate-exports": "node scripts/generate-exports.js",
    "dev": "vite build --watch",
    "clean": "rm -rf dist exports.json",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-onboarding": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-storybook": "^9.1.2",
    "storybook": "^9.1.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "design-system",
    "ui-components",
    "react",
    "typescript",
    "peak-health",
    "fitness",
    "health"
  ],
  "author": "Peak Health Team",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./button": {
      "types": "./dist/components/button/index.d.ts",
      "import": "./dist/components/button/index.js",
      "require": "./dist/components/button/index.js"
    },
    "./input": {
      "types": "./dist/components/input/index.d.ts",
      "import": "./dist/components/input/index.js",
      "require": "./dist/components/input/index.js"
    },
    "./modal": {
      "types": "./dist/components/modal/index.d.ts",
      "import": "./dist/components/modal/index.js",
      "require": "./dist/components/modal/index.js"
    },
    "./tooltip": {
      "types": "./dist/components/tooltip/index.d.ts",
      "import": "./dist/components/tooltip/index.js",
      "require": "./dist/components/tooltip/index.js"
    }
  }
}
